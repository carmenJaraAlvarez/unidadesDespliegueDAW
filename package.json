{
  "name": "unidades-page",
  "version": "1.0.0",
  "description": "Educational web application for displaying units content",
  "main": "src/main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "verbose": true,
    "transform": {},
    "testEnvironmentOptions": {
      "html": "<!DOCTYPE html><html><head></head><body></body></html>"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carmenJaraAlvarez/unidadesDespliegueDAW.git"
  },
  "bugs": {
    "url": "https://github.com/carmenJaraAlvarez/unidadesDespliegueDAW/issues"
  },
  "homepage": "https://github.com/carmenJaraAlvarez/unidadesDespliegueDAW#readme"
}
